{
  "extends": [
    "tslint-config-prettier"
  ],
  "rules": {
    "no-submodule-imports": false,
    "object-literal-shorthand": true,
    "quotemark": [
      true,
      "single",
      "jsx-single"
    ],
    "whitespace": [
      true,
      "check-branch",
      "check-decl",
      "check-operator",
      "check-module",
      "check-rest-spread"
    ],
    "newline-before-return": true,
    "no-unnecessary-callback-wrapper": false,
    "no-this-assignment": false
  }
}